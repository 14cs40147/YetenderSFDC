global class Calculator {

    webservice static Integer add(Integer p1,Integer p2)
    {
        return p1+p2;
    }
    webservice static Integer sub(Integer p1,Integer p2)
    {
        return p1-p2;
    }
    webservice static Integer mul(Integer p1,Integer p2)
    {
        return p1*p2;
    }
    webservice static Integer div(Integer p1,Integer p2)
    {
        return p1/p2;
    }
    
}